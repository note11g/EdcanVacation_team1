<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="8dp"
            android:fontFamily="@font/nanumsquareb"
            android:text="#실시간 업로드"
            android:textColor="@color/materialBlack"
            android:textSize="26sp"
            app:layout_constraintBottom_toTopOf="@+id/view"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nanumsquarer"
            android:text="더보기"
            android:textColor="@color/materialGray7"
            android:textSize="14dp"
            app:layout_constraintBottom_toTopOf="@+id/view"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="8dp"/>

        <View
            android:id="@+id/view"
            android:layout_width="360dp"
            android:layout_height="0.5dp"
            android:layout_marginTop="80dp"
            android:background="@color/materialBlack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view1"
            android:layout_width="360dp"
            android:layout_height="0.3dp"
            android:layout_marginTop="8dp"
            android:background="@color/materialBlack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/live_txt1" />

        <Button
            android:id="@+id/live_txt1"
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:background="@color/invisible"
            android:fontFamily="@font/nanumsquareb"
            android:gravity="fill"
            android:padding="8dp"
            android:layout_marginTop="8dp"
            android:text="[넷플릭스] [2인] [1개월]"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view" />

        <View
            android:id="@+id/view2"
            android:layout_width="360dp"
            android:layout_height="0.3dp"
            android:layout_marginTop="8dp"
            android:background="@color/materialBlack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/live_txt2" />

        <Button
            android:id="@+id/live_txt2"
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:background="@color/invisible"
            android:fontFamily="@font/nanumsquareb"
            android:gravity="fill"
            android:layout_marginTop="8dp"
            android:padding="8dp"
            android:text="[웨이브] [3인] [5개월]"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view1" />

        <View
            android:id="@+id/view3"
            android:layout_width="360dp"
            android:layout_height="0.3dp"
            android:layout_marginTop="8dp"
            android:background="@color/materialBlack"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/live_txt3" />

        <Button
            android:id="@+id/live_txt3"
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:background="@color/invisible"
            android:fontFamily="@font/nanumsquareb"
            android:layout_marginTop="8dp"
            android:gravity="fill"
            android:padding="8dp"
            android:text="[유튜브 프리미엄] [5인] [8개월]"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view2" />

        <TextView
            android:id="@+id/hot_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nanumsquareb"
            android:text="#요즘 가장 핫한"
            android:textColor="@color/materialBlack"
            android:textSize="26sp"
            android:layout_marginStart="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="370dp"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/hot_img1"
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="8dp"
            android:elevation="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hot_txt"
            android:src="@drawable/hot_img1"/>

        <FrameLayout
            android:id="@+id/frameLayout2"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hot_img1">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:background="@android:color/white"
                android:fontFamily="@font/nanumsquareb"
                android:padding="4dp"
                android:text="비밀의 숲"
                android:textColor="@color/materialBlack"
                android:textSize="12sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:fontFamily="@font/nanumsquarer"
                android:padding="4dp"
                android:text="넷플릭스"
                android:textColor="@color/materialGray6"
                android:textSize="8dp" />


        </FrameLayout>

        <ImageView
            android:id="@+id/hot_img2"
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:layout_marginTop="8dp"
            android:layout_marginStart="30dp"
            android:elevation="8dp"
            android:src="@drawable/hot_img2"
            app:layout_constraintStart_toEndOf="@+id/hot_img1"
            app:layout_constraintTop_toBottomOf="@+id/hot_txt" />

        <FrameLayout
            android:id="@+id/frameLayout3"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            app:layout_constraintStart_toEndOf="@+id/frameLayout2"
            app:layout_constraintTop_toBottomOf="@+id/hot_img2">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:background="@android:color/white"
                android:fontFamily="@font/nanumsquareb"
                android:padding="4dp"
                android:text="달의연인 \n보보경심려"
                android:textColor="@color/materialBlack"
                android:textSize="12sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:fontFamily="@font/nanumsquarer"
                android:padding="4dp"
                android:text="넷플릭스"
                android:textColor="@color/materialGray6"
                android:textSize="8dp" />


        </FrameLayout>

        <ImageView
            android:id="@+id/hot_img3"
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:layout_marginTop="8dp"
            android:layout_marginStart="30dp"
            android:elevation="8dp"
            android:src="@drawable/hot_img3"
            app:layout_constraintStart_toEndOf="@+id/hot_img2"
            app:layout_constraintTop_toBottomOf="@+id/hot_txt" />

        <FrameLayout
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            app:layout_constraintStart_toEndOf="@+id/frameLayout3"
            app:layout_constraintTop_toBottomOf="@+id/hot_img3">

            <TextView
                android:id="@+id/textView12"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:background="@android:color/white"
                android:fontFamily="@font/nanumsquareb"
                android:padding="4dp"
                android:text="사랑의 불시착"
                android:textColor="@color/materialBlack"
                android:textSize="12sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:fontFamily="@font/nanumsquarer"
                android:padding="4dp"
                android:text="넷플릭스"
                android:textColor="@color/materialGray6"
                android:textSize="8dp" />


        </FrameLayout>

        <ImageView
            android:id="@id/hot_img4"
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:elevation="8dp"
            android:layout_marginStart="40dp"
            android:src="@drawable/hot_img4"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout2" />

        <FrameLayout
            android:id="@+id/frameLayout4"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/hot_img4">

            <TextView
                android:id="@+id/textView13"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:background="@android:color/white"
                android:fontFamily="@font/nanumsquareb"
                android:padding="4dp"
                android:text="사이코지만 \n괜찮아"
                android:textColor="@color/materialBlack"
                android:textSize="12sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:fontFamily="@font/nanumsquarer"
                android:padding="4dp"
                android:text="넷플릭스"
                android:textColor="@color/materialGray6"
                android:textSize="8dp" />


        </FrameLayout>

        <ImageView
            android:id="@+id/hot_img5"
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:elevation="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginStart="30dp"
            android:src="@drawable/hot_img5"
            app:layout_constraintStart_toEndOf="@+id/hot_img4"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout3" />

        <FrameLayout
            android:id="@+id/frameLayout5"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            app:layout_constraintStart_toEndOf="@+id/frameLayout4"
            app:layout_constraintTop_toBottomOf="@+id/hot_img5">

            <TextView
                android:id="@+id/textView14"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:background="@android:color/white"
                android:fontFamily="@font/nanumsquareb"
                android:padding="4dp"
                android:text="연애참견"
                android:textColor="@color/materialBlack"
                android:textSize="12sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:fontFamily="@font/nanumsquarer"
                android:padding="4dp"
                android:text="넷플릭스"
                android:textColor="@color/materialGray6"
                android:textSize="8dp" />


        </FrameLayout>

        <ImageView
            android:id="@+id/hot_img6"
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:elevation="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginStart="30dp"
            android:src="@drawable/hot_img6"
            app:layout_constraintStart_toEndOf="@+id/hot_img5"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout3" />

        <FrameLayout
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            app:layout_constraintStart_toEndOf="@+id/frameLayout5"
            app:layout_constraintTop_toBottomOf="@+id/hot_img6">

            <TextView
                android:id="@+id/textView15"
                android:layout_width="90dp"
                android:layout_height="50dp"
                android:background="@android:color/white"
                android:fontFamily="@font/nanumsquareb"
                android:padding="4dp"
                android:text="킹덤"
                android:textColor="@color/materialBlack"
                android:textSize="12sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:fontFamily="@font/nanumsquarer"
                android:padding="4dp"
                android:text="넷플릭스"
                android:textColor="@color/materialGray6"
                android:textSize="8dp" />


        </FrameLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>